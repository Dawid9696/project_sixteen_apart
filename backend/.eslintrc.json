{
	"extends": ["airbnb", "prettier", "plugin:node/recommended"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error",
		"semi": ["error", "always"],
		"quotes": ["error", "single"],
		"no-unused-vars": "warn",
		"no-console": "off",
		"no-process-exit": "off",
		"func-names": "off"
	}
}
